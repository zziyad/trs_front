        },
        "arrivalLocation": {
          "type": "string",
          "required": true,
          "description": "Arrival location"
        },
        "departureTime": {
          "type": "date",
          "required": true,
          "description": "Scheduled departure time"
        },
        "arrivalTime": {
          "type": "date",
          "required": true,
          "description": "Scheduled arrival time"
        },
        "vehicleId": {
          "type": "string",
          "required": false,
          "description": "Assigned vehicle ID"
        },
        "driverId": {
          "type": "string",
