          "description": "Assigned driver ID"
        },
        "passengerCount": {
          "type": "number",
          "required": true,
          "min": 0,
          "description": "Number of passengers"
        },
        "maxCapacity": {
          "type": "number",
          "required": true,
          "min": 1,
          "description": "Maximum passenger capacity"
        },
        "status": {
          "type": "enum",
          "required": false,
          "default": "scheduled",
          "values": ["scheduled", "in-progress", "completed", "cancelled", "delayed"],
