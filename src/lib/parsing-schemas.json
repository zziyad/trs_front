        },
        "employeeId": {
          "type": "string",
          "required": true,
          "description": "Employee ID"
        },
        "firstName": {
          "type": "string",
          "required": true,
          "description": "Driver first name"
        },
        "lastName": {
          "type": "string",
          "required": true,
          "description": "Driver last name"
        },
        "dateOfBirth": {
          "type": "date",
          "required": true,
          "description": "Driver date of birth"
        },
        "licenseNumber": {
          "type": "string",
          "required": true,
          "description": "Driver license number"
        },
        "licenseType": {
          "type": "enum",
          "required": true,
          "values": ["A", "B", "C", "D", "E"],
          "description": "License type"
        },
        "licenseExpiry": {
          "type": "date",
          "required": true,
          "description": "License expiry date"
        },
        "experienceYears": {
          "type": "number",
          "required": true,
          "min": 0,
          "description": "Years of driving experience"
        },
        "contactPhone": {
          "type": "string",
          "required": true,
          "description": "Contact phone number"
        },
        "contactEmail": {
          "type": "email",
          "required": true,
          "description": "Contact email"
        },
        "emergencyContact": {
          "type": "object",
          "required": true,
          "fields": {
            "name": "string",
            "relationship": "string",
            "phone": "string",
            "email": "email"
          },
          "description": "Emergency contact information"
        },
        "medicalCertificate": {
          "type": "object",
          "required": false,
          "fields": {
            "number": "string",
            "expiryDate": "date",
            "issuingAuthority": "string"
          },
          "description": "Medical certificate details"
        },
        "status": {
          "type": "enum",
          "required": false,
          "default": "active",
          "values": ["active", "inactive", "suspended", "on-leave"],
          "description": "Driver status"
        },
        "assignedVehicle": {
          "type": "string",
          "required": false,
          "description": "Currently assigned vehicle ID"
        },
        "currentLocation": {
          "type": "string",
          "required": false,
          "description": "Current driver location"
        }
      }
    },
