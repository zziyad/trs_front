{
  "schemas": {
    "BaseParsedData": {
      "description": "Base schema for all parsed data with common fields",
      "fields": {
        "id": {
          "type": "string",
          "required": false,
          "description": "Optional unique identifier"
        },
        "source": {
          "type": "string",
          "required": true,
          "description": "Source of the parsed data"
        },
        "timestamp": {
          "type": "date",
          "required": false,
          "default": "current_date",
          "description": "When the data was parsed"
        },
        "rawData": {
          "type": "string",
          "required": true,
          "description": "Original raw data before parsing"
        },
        "parsedAt": {
          "type": "date",
          "required": false,
          "default": "current_date",
          "description": "When parsing completed"
        },
        "confidence": {
          "type": "number",
          "required": false,
          "default": 0.8,
          "min": 0,
          "max": 1,
          "description": "Confidence score of parsing accuracy"
        },
        "errors": {
          "type": "array",
          "required": false,
          "default": [],
          "items": "string",
          "description": "Any parsing errors encountered"
        },
        "warnings": {
          "type": "array",
          "required": false,
          "default": [],
          "items": "string",
          "description": "Any parsing warnings"
        }
      }
    },
    "Event": {
      "description": "Event data parsing schema",
      "extends": "BaseParsedData",
      "fields": {
        "eventId": {
          "type": "string",
          "required": true,
          "description": "Unique event identifier"
        },
        "eventName": {
          "type": "string",
          "required": true,
          "description": "Name of the event"
        },
        "eventType": {
          "type": "enum",
          "required": true,
          "values": ["conference", "meeting", "travel", "training", "other"],
          "description": "Type of event"
        },
        "startDate": {
          "type": "date",
          "required": true,
          "description": "Event start date"
        },
        "endDate": {
          "type": "date",
          "required": true,
          "description": "Event end date"
        },
        "location": {
          "type": "string",
          "required": false,
          "description": "Event location"
        },
        "description": {
          "type": "string",
          "required": false,
          "description": "Event description"
        },
        "status": {
          "type": "enum",
          "required": false,
          "default": "planned",
          "values": ["planned", "active", "completed", "cancelled"],
          "description": "Event status"
        },
        "priority": {
          "type": "enum",
          "required": false,
          "default": "medium",
          "values": ["low", "medium", "high", "critical"],
          "description": "Event priority"
        },
        "attendees": {
          "type": "array",
          "required": false,
          "default": [],
          "items": "string",
          "description": "List of attendee IDs"
        },
        "budget": {
          "type": "number",
          "required": false,
          "description": "Event budget"
        },
        "organizer": {
          "type": "string",
          "required": true,
          "description": "Event organizer ID"
        }
      }
    },
    "EventAccommodation": {
      "description": "Event accommodation parsing schema",
      "extends": "BaseParsedData",
      "fields": {
        "eventId": {
          "type": "string",
          "required": true,
          "description": "Associated event ID"
        },
        "accommodationId": {
          "type": "string",
          "required": true,
          "description": "Unique accommodation identifier"
        },
        "hotelName": {
          "type": "string",
          "required": true,
          "description": "Hotel name"
        },
        "address": {
          "type": "string",
          "required": true,
          "description": "Hotel address"
        },
        "checkInDate": {
          "type": "date",
          "required": true,
          "description": "Check-in date"
        },
        "checkOutDate": {
          "type": "date",
          "required": true,
          "description": "Check-out date"
        },
        "roomType": {
          "type": "string",
          "required": true,
          "description": "Type of room"
        },
        "roomNumber": {
          "type": "string",
          "required": false,
          "description": "Room number"
        },
        "guestCount": {
          "type": "number",
          "required": true,
          "min": 1,
          "description": "Number of guests"
        },
        "status": {
          "type": "enum",
          "required": true,
          "values": ["reserved", "confirmed", "checked-in", "checked-out", "cancelled"],
          "description": "Accommodation status"
        },
        "cost": {
          "type": "number",
          "required": false,
          "description": "Accommodation cost"
        },
        "specialRequests": {
          "type": "string",
          "required": false,
          "description": "Special accommodation requests"
        }
      }
    },
    "Passenger": {
      "description": "Passenger data parsing schema",
      "extends": "BaseParsedData",
      "fields": {
        "passengerId": {
          "type": "string",
          "required": true,
          "description": "Unique passenger identifier"
        },
        "firstName": {
          "type": "string",
          "required": true,
          "description": "Passenger first name"
        },
        "lastName": {
          "type": "string",
          "required": true,
          "description": "Passenger last name"
        },
        "dateOfBirth": {
          "type": "date",
          "required": true,
          "description": "Passenger date of birth"
        },
        "nationality": {
          "type": "string",
          "required": true,
          "description": "Passenger nationality"
        },
        "passportNumber": {
          "type": "string",
          "required": false,
          "description": "Passport number"
        },
        "passportExpiry": {
          "type": "date",
          "required": false,
          "description": "Passport expiry date"
        },
        "visaNumber": {
          "type": "string",
          "required": false,
          "description": "Visa number"
        },
        "visaExpiry": {
          "type": "date",
          "required": false,
          "description": "Visa expiry date"
        },
        "contactPhone": {
          "type": "string",
          "required": false,
          "description": "Contact phone number"
        },
        "contactEmail": {
          "type": "email",
          "required": false,
          "description": "Contact email"
        },
        "emergencyContact": {
          "type": "object",
          "required": false,
          "fields": {
            "name": "string",
            "relationship": "string",
            "phone": "string",
            "email": "email"
          },
          "description": "Emergency contact information"
        },
        "dietaryRestrictions": {
          "type": "array",
          "required": false,
          "default": [],
          "items": "string",
          "description": "Dietary restrictions"
        },
        "medicalConditions": {
          "type": "array",
          "required": false,
          "default": [],
          "items": "string",
          "description": "Medical conditions"
        },
        "specialAssistance": {
          "type": "boolean",
          "required": false,
          "default": false,
          "description": "Requires special assistance"
        },
        "status": {
          "type": "enum",
          "required": false,
          "default": "active",
          "values": ["active", "inactive", "suspended"],
          "description": "Passenger status"
        }
      }
    },
    "Vehicle": {
      "description": "Vehicle data parsing schema",
      "extends": "BaseParsedData",
      "fields": {
        "vehicleId": {
          "type": "string",
          "required": true,
          "description": "Unique vehicle identifier"
        },
        "registrationNumber": {
          "type": "string",
          "required": true,
          "description": "Vehicle registration number"
        },
        "make": {
          "type": "string",
          "required": true,
          "description": "Vehicle make"
        },
        "model": {
          "type": "string",
          "required": true,
          "description": "Vehicle model"
        },
        "year": {
          "type": "number",
          "required": true,
          "min": 1900,
          "max": "current_year + 1",
          "description": "Vehicle year"
        },
        "color": {
          "type": "string",
          "required": true,
          "description": "Vehicle color"
        },
        "capacity": {
          "type": "number",
          "required": true,
          "min": 1,
          "description": "Passenger capacity"
        },
        "fuelType": {
          "type": "enum",
          "required": true,
          "values": ["petrol", "diesel", "electric", "hybrid", "other"],
          "description": "Fuel type"
        },
        "transmission": {
          "type": "enum",
          "required": true,
          "values": ["manual", "automatic"],
          "description": "Transmission type"
        },
        "status": {
          "type": "enum",
          "required": false,
          "default": "available",
          "values": ["available", "in-use", "maintenance", "out-of-service"],
          "description": "Vehicle status"
        },
        "lastMaintenance": {
          "type": "date",
          "required": false,
          "description": "Last maintenance date"
        },
        "nextMaintenance": {
          "type": "date",
          "required": false,
          "description": "Next maintenance due date"
        },
        "mileage": {
          "type": "number",
          "required": true,
          "min": 0,
          "description": "Current mileage"
        },
        "fuelLevel": {
          "type": "number",
          "required": false,
          "min": 0,
          "max": 100,
          "description": "Current fuel level percentage"
        },
        "location": {
          "type": "string",
          "required": false,
          "description": "Current vehicle location"
        },
        "assignedDriver": {
          "type": "string",
          "required": false,
          "description": "Currently assigned driver ID"
        }
      }
    },
    "Driver": {
      "description": "Driver data parsing schema",
